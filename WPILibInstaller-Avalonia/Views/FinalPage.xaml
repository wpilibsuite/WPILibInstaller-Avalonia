<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="WPILibInstaller.Views.FinalPage">
  <StackPanel VerticalAlignment="Center">
    <TextBlock FontSize="20" HorizontalAlignment="Center" VerticalAlignment="Center">
      Finished installing! Press finish to exit.
    </TextBlock>
  <TextBlock FontSize="20" HorizontalAlignment="Center" VerticalAlignment="Center" Text="{Binding FinishText}">
  </TextBlock>
  </StackPanel>

</UserControl>
